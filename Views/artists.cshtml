<h1>Current Artists</h1>

@if(@Model.Count == 0)
{
  <h3>No Artists have been added yet!</h3>
}

@foreach(var artist in Model)
{
  <h3><a href="/artists/@artist.GetId()">@artist.GetArtist()</a></h3>
}

<h3> <a href="/cd/new">Add a new CD</a></h3>
<h3><a href="/artist/search">Search Artists</a></h3>
